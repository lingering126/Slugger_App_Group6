{
  "expo": {
    "name": "Slugger",
    "slug": "slugger",
    "scheme": "slugger",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "static",
      "name": "Slugger Web",
      "description": "Slugger App for Web Browsers",
      "themeColor": "#ffffff",
      "backgroundColor": "#ffffff",
      "build": {
        "babel": {
          "include": [
            "@expo/vector-icons",
            "react-native-vector-icons",
            "react-native-paper",
            "react-native-elements",
            "react-native"
          ]
        }
      }
    },
    "plugins": [
      "expo-router"
    ]
  }
}
